/* pages/cs/cs.wxss */
page {
    background-color: #f0f2f5;
}
.cs-top{
  background-color: #466df0;
  color: #fff;
  padding: 10px 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 111;
}
.cs-top-left{
  text-align: center;
  font-size: 12pt;
}
.cs-top-right{
  text-align: center;
  font-size: 10pt;
}
.cs-nav{
  margin: 10pt;
  padding: 8pt;
  background-color: #fff;
}
.cs-nav-box{
  display: flex;
  padding-bottom: 8pt;
  border-bottom: 1px solid #F0F0F0;
}
.cs-nav-image{
  width: 66pt; 
  height: 66pt;
  border-radius: 5pt;
}
.cs-nav-icon{
  width: 10pt; 
  height: 10pt;
  margin-right: 3pt;
}
.cs-nav-right{
  padding-left: 10pt;
  width: 230pt;
  height: 66pt;
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
}
.cs-nav-right .top{
 display: flex;
 width: 100%;
 justify-content: space-between;  
 align-items: center;  
}
.cs-nav-right .top .right{
  font-size: 10pt;
}
.cs-nav-right .top .left{
  font-size: 12pt;
}
.address{
  font-size: 10pt;
  color: #BFBFBF;
}
.time{
  font-size: 10pt;
  color: #656E6D;
}
.cs-nav-bottom{
  display: flex;
  padding-top: 8pt;
}
.cs-nav-bottom .nav{
  width: 50%;
  font-size: 11pt;
  color: #434343;
}